generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model Blog {
  id String @id @default(auto()) @map("_id") @db.ObjectId
}

model PropertyLoss {
  id String @id @default(auto()) @map("_id") @db.ObjectId
}

model Quiz {
  id String @id @default(auto()) @map("_id") @db.ObjectId
}

model User {
  id    String @id @default(auto()) @map("_id") @db.ObjectId
  /// Field referred in an index, but found no data to define the type.
  phone Json?  @unique
}
